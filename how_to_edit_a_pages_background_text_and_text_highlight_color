   <body text = "#FFCB05" bgcolor = "#00274C">
      
      <p>Hex palette: Michigan University</p>
      
      	<table bgcolor = "#FFCB05">
      
         <tr>
            <td>
        <font color = "#00274C">This text will appear white on black background.
        </font>
            </td>
         </tr>
      </table>
   </body>
   
